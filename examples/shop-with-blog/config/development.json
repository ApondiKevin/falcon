  "sessionStore": {
    "type": "memory"
  },
  "store": {
    "demo": true,
    "enableSwitcher": true,
    "enableAutoDetection": false,
    "mapping": {
      "*": "default",
      "NL": "nl_store",
      "PL": "pl_store",
      "FR": "fr_store",
      "DE": "de_store"
