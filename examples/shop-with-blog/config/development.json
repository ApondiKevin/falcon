  "wpApiProtocol": "https",
  "wpApiPrefix": "/blog-api",
  "reduxDevtoolsEnable": true,
  "useCache": true,
  "cacheBackend": "memory",
  "graphiql": true,
  "magento2": {
    "cacheToken": true
  },
  "title": "Deity 1.3.0 alpha",
  "extensions": ["shop", "blog", "url", "wordpress", "magento2"],
  "head": {
    "title": "Deity 1.3.0 alpha"
