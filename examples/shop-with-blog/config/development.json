    "languageMapping": {
      "en": ["*"],
      "nl": ["nl_store"],
      "pl": ["pl_store"],
      "de": ["de_store"],
      "fr": ["fr_store"]
    },
    "codeMapping": {
      "default": "default",
      "nl_store": "nl_store",
      "pl_store": "pl_store",
      "fr_store": "fr_store",
      "de_store": "de_store"
    }
  },
  "languages": {
    "all": ["en", "nl", "pl", "de", "fr"],
    "main": "en",
    "namespaces": ["common", "blog", "shop", "account"]
  },
  "currency": {
    "symbol": "GBP",
    "country": "en-GB",
    "trimDecimal": true,
    "mapping": {
      "EUR": "en-IE",
      "USD": "en-GB",
      "GBP": "en-GB",
