  "scripts": {
    "start": "concurrently --raw --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --raw --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "stats": "webpack --profile --json > webpack-stats.json --config webpack/prod.config.js",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "betterScripts": {
    "start-prod": {
      "command": "cross-env NODE_OPTIONS=$NODE_DEBUG_OPTION node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "cross-env NODE_OPTIONS=$NODE_DEBUG_OPTION node --inspect ./bin/server.js",
      "env": {
        "BABEL_DISABLE_CACHE": 1,
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --colors --display-error-details --config=webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
