{
  "name": "@deity/falcon-server",
  "license": "OSL-3.0",
  "version": "0.2.2",
  "homepage": "https://falcon.deity.io/docs/falcon-server/basics",
  "repository": "https://github.com/deity-io/falcon/tree/master/packages/falcon-server",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "falcon-scripts clean",
    "build": "falcon-scripts build --target=NODE",
    "watch": "falcon-scripts watch --target=NODE",
    "test": "falcon-scripts test",
    "test:coverage": "falcon-scripts test:coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "html",
      "text",
      "text-summary",
      "lcov"
    ],
    "setupFiles": [
      "../../test/nodeEnvSetupFile"
    ]
  },
  "dependencies": {
    "@deity/falcon-errors": "^0.1.2",
    "@deity/falcon-logger": "^0.1.2",
    "@deity/falcon-server-env": "^0.1.5",
    "@koa/cors": "2.2.2",
    "apollo-server-cache-memcached": "0.3.1",
    "apollo-server-cache-redis": "0.3.1",
    "apollo-server-caching": "0.3.1",
    "apollo-server-env": "2.2.0",
    "apollo-server-koa": "2.4.8",
    "eventemitter2": "5.0.1",
    "graphql": "^14.1.1",
    "graphql-tools": "4.0.4",
    "graphql-type-json": "0.3.0",
    "koa": "2.7.0",
    "koa-body": "4.1.0",
    "koa-router": "7.4.0",
    "koa-session": "5.12.0",
    "lodash": "4.17.11"
  },
  "devDependencies": {
    "@types/graphql-type-json": "0.3.0",
    "@types/koa-session": "5.10.0",
    "@deity/falcon-scripts": "^0.0.1",
    "supertest": "3.4.2"
  },
  "gitHead": "d8cd5eede81220aa29563275dcf35448611cc194"
}
